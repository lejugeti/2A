<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="Domain"
namespace="Domain">
	<class name="Prospect" table="prospect">
		<id name="Id" column="prosp_id" type="int">
			<generator class="increment"></generator>
		</id>

		<property name="LastName" column="prosp_last_name" type="String"/>
		<property name="FirstName" column="prosp_first_name" type="String"/>
		<property name="Email" column="prosp_mail" type="String"/>
		<property name="LastContact" column="prosp_last_contact" type="Date"/>
		<property name="Notes" column="prosp_notes" type="String"/>
		
		
	</class>
</hibernate-mapping>